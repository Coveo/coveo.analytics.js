{
  "name": "coveo.analytics",
  "version": "0.2.0",
  "description": "ðŸ“ˆ Coveo analytics client (node and browser compatible) ",
  "main": "dist/index.js",
  "browser": "dist/bundle.js",
  "scripts": {
    "lint": "tslint -s \"node_modules/tslint-loader/formatters/\" \"test/**/*.ts\" \"src/**/*.ts\"",
    "build:webpack": "webpack",
    "build:tsc": "tsc",
    "build": "npm run-script lint && npm run-script build:webpack && npm run-script build:tsc",
    "test": "tsc -p tsconfig.test.json && nyc ava \"**/*test.js\"",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist/*"
  },
  "author": "Pierre-Alexandre St-Jean <pastjean@coveo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/coveo/coveo.analytics.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "0.16.0",
    "body-parser": "1.15.1",
    "coveralls": "2.11.14",
    "es6-promise": "4.0.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "9.5.0",
    "nyc": "8.3.0",
    "sinon": "1.17.6",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.3",
    "webpack": "1.13.2"
  },
  "dependencies": {},
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "src/**/*.ts",
    "LICENSE"
  ],
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ]
  }
}

